<table class="metadata table table-striped table-condensed">

  <thead class="header">
    <tr>
      <td><h4>Key</h4></td>
      <td><h4>Value</h4></td>
    </tr>
  </thead>

  <tbody>
    <% _.each(metadata, function(md) { %>
    <tr title="<%= md.key %>" data-content="<%= md.value %>">
      <td><%= md.key %></td>
      <td><%= md.value %></td>
    </tr>
    <% }); %>
  </tbody>

</table>
