
<tr>
  <td><a><%= scheduled_command.command %></a></td>
  <td><%= scheduled_command.owner %></td>
  <td class="text-center"><%= schedule_type() %></td>
  <td><%= schedule() %></td>
  <td>
    <%= date(scheduled_command.last_run) || "" %>  <%= status() %>
  </td>
  <td>
    <%= date(scheduled_command.next_run) || "n/a" %>
  </td>
</tr>
