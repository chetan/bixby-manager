
<h1>Team</h1>

<p>
  <a class="btn btn-sm btn-primary add_user" role="button">Add User</a>
</p>

<table class="table table-bordered team">
  <thead>
    <tr>
      <th>Name</th>
      <th>Username</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Created</th>
      <th>Status</th>
      <th>Last Login</th>
    </tr>
  </thead>
  <tbody>
    <%= each_partial(users, "user", Bixby.view.TeamRow) %>
  </tbody>
</table>
