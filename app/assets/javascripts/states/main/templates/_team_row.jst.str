
<tr>
  <td><a class="name"></a></td>
  <td><a class="username"></a></td>
  <td><a class="email" href="mailto:<%= user.email %>"></a></td>
  <td class="phone"></td>
  <td class="created_at"><%= format_datetime(user.created_at) %></td>
  <td class="status"></td>

  <%? user.last_sign_in_at %>
    <td class="last_login" title="<%= format_datetime(user.last_sign_in_at) %>">
      <%= user.last_sign_in_at.fromNow() %>
    </td>
  <% } else { %>
    <td class="last_login"></td>
  <%/%>
</tr>
