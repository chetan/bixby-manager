
<h2>
  Add Repository
</h2>

<br>
<form class="form-horizontal">

  <!-- name -->
  <%= include("main/_text_input", {name: "name"}) %>

  <!-- uri -->
  <%= include("main/_text_input", {name: "uri", label: "URI"}) %>

  <!-- branch -->
  <%= include("main/_text_input", {name: "branch"}) %>

  <!-- requires key -->
  <%= include("main/_text_input", {name: "requires_key", type: "checkbox"}) %>

  <!-- submit -->
  <div class="control-group">
    <div class="controls">
      <button type="submit" class="btn btn-primary submit">Add</button>
    </div>
  </div>

</form>
