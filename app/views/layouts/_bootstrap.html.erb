  <script type="text/javascript">
    Bixby.app.bootstrap({
    <%
    @bootstrap.each do |boot|
      if boot[:model] =~ /List$/ or boot[:data].kind_of? Array then %>
        <%= boot[:name] %>: (new Bixby.model.<%= boot[:model] %>()).reset(<%= boot[:data] %>),
      <% else %>
        <%= boot[:name] %>: new Bixby.model.<%= boot[:model] %>(<%= boot[:data] %>),
      <%
      end
    end %>
     });
  </script>
